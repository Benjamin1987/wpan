<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="android:Theme.Holo.Light.DarkActionBar">
        <item name="android:windowNoTitle">true</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="navigationbar_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">49dp</item>
        <!--<item name="android:background">@drawable/navigationbar_bg</item>-->
        <item name="android:background">#333236</item>
    </style>

    <style name="SwipeBackLayout">
        <item name="edge_size">20dp</item>
        <item name="shadow_left">@mipmap/shadow_left</item>
        <item name="shadow_right">@mipmap/shadow_right</item>
        <item name="shadow_bottom">@mipmap/shadow_bottom</item>
    </style>

</resources>
