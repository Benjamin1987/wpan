<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/default_bg"
    android:orientation="vertical">

    <include layout="@layout/ly_title_bar" />

    <LinearLayout
        android:id="@+id/rootLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:visibility="gone">

        <com.xinyu.mwp.view.CellView
            android:id="@+id/cashBank"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_15"
            app:cell_arrow_visible="false"
            app:cell_content_color="@color/font_666"
            app:cell_content_size="@dimen/fontSize_16sp"
            app:cell_icon_visible="false"
            app:cell_name="提现银行" />

        <com.xinyu.mwp.view.CellView
            android:id="@+id/cashTime"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cell_arrow_visible="false"
            app:cell_content_color="@color/font_666"
            app:cell_content_size="@dimen/fontSize_16sp"
            app:cell_icon_visible="false"
            app:cell_name="提现时间" />

        <com.xinyu.mwp.view.CellView
            android:id="@+id/maybeCashGetTime"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cell_arrow_visible="false"
            app:cell_content_color="@color/font_666"
            app:cell_content_size="@dimen/fontSize_16sp"
            app:cell_icon_visible="false"
            app:cell_name="预计到账时间" />

        <com.xinyu.mwp.view.CellView
            android:id="@+id/cashGetTime"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cell_arrow_visible="false"
            app:cell_content_color="@color/default_red"
            app:cell_content_size="@dimen/fontSize_16sp"
            app:cell_icon_visible="false"
            app:cell_name="到账时间" />
    </LinearLayout>

</LinearLayout>